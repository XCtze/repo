local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Load whitelist
local success, Whitelist = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/XCtze/repo/refs/heads/main/wl", true))()
end)

if not success or type(Whitelist) ~= "table" then
    LocalPlayer:Kick("Failed to load whitelist.")
    return
end

-- Check if the player is whitelisted
if table.find(Whitelist, LocalPlayer.UserId) then
    print("Whitelisted! Running script...")

    -- Place the script you want to execute here:
    loadstring(game:HttpGet("https://pastebin.com/raw/YOUR_SCRIPT", true))()
else
    LocalPlayer:Kick("You are not whitelisted!")
end
